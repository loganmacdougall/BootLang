def calc(l, r, o):
    if o == "+":
        a = (l + r)
    elif o == "-":
        a = (l - r)
    elif o == "*":
        a = (l * r)
    elif o == "/":
        a = (l / r)
    
    print(l, o, r, "=", a)

calc(4, 8, "*")
calc(3, 34, "+")
calc(44, 55, "-")
calc(44, -55, "+")
calc(-44, -55, "*")
calc(-44, 55, "/")
calc(-44.3, 5.2, "/")
calc(-44.3, 5.2, "+")
calc(-4.8, 9.2, "-")
calc(-4, 9, "/")
calc(-8.0, 3, "*")